{
  "name": "home",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node ./build.js",
    "watch": "node ./watch.js",
    "lint:scss": "stylelint ./wesgarlock/**/static_src/*.scss ./wesgarlock/**/static_src/scss/** --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix",
    "lint:js": "eslint ./wesgarlock/**/static_src/*.js ./wesgarlock/**/static_src/js/** --ext .js --ignore-path ./.eslintignore",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "aos": "^2.3.4",
    "classnames": "^2.2.6",
    "css": "^3.0.0",
    "date-fns": "^2.16.1",
    "dompurify": "^2.0.12",
    "gulp": "^4.0.2",
    "html-react-parser": "^0.13.0",
    "js": "^0.1.0",
    "js-cookie": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intersection-observer": "^8.26.2",
    "react-token-auth": "^1.1.7"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.5",
    "less": "^3.12.2",
    "lint-staged": "^10.5.3",
    "pre-commit": "^1.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./wesgarlock/**/static_src/js/**": [
      "eslint ./wesgarlock/**/static_src/js/** --fix --ignore-path ./.eslintignore", "git add"
    ],
    "./wesgarlock/**/static_src/*.{js}": [
      "eslint ./wesgarlock/**/static_src/*.js --fix --ignore-path ./.eslintignore", "git add"
    ],
    "./wesgarlock/**/static_src/scss/** *.scss": [
      "stylelint --syntax scss --fix", "git add"
    ],
    "./wesgarlock/**/static_src/*.{scss}": [
      "stylelint --syntax scss --fix", "git add"
    ]
  }
}
