<!DOCTYPE html>
<html lang="en">
<head>
  {% block head %}
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" />
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons" />
  <title itemprop="name">
    {% block title %}
        {% if page.seo_title %}{{ page.seo_title }}{% else %}{{ page.title }}{% endif %}
    {% endblock %}
</title>
{% endblock%}
  {% block opengraph %}
  {% endblock %}
  
  <link rel="icon" type="image/png" href='{{ static("image/favicon.ico") }}'/> 
  
  {% block extra_css%}
  {% endblock %}
</head>
<body class="{% block body_class %}{% endblock %}">

  {% block content %}{% with page=page.specific %}
    <div id="root"></div>
  {% endwith %}{% endblock %}

  {% block context%}
    {% set base_context_id = "base_context" %}
    {{ base_context|dump(base_context_id, "context") }}
    <script>
      var csrftoken = "{{ csrf_token }}"
      var csrf_input = "{% csrf_token %}"
    </script>
  {% endblock %}
  
  {% block extra_js %}
    <script src="{{ static('wesgarlock-base.js') }}" ></script>
  {% endblock %}
</body>
</html>